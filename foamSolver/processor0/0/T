FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273;

boundaryField
{
    leftWall
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    rightWall
    {
        type            fixedValue;
        value           uniform 273;
    }
    topWall
    {
        type            fixedValue;
        value           uniform 273;
    }
    bottomWall
    {
        type            fixedValue;
        value           uniform 273;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 273;
    }
}



thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    properties      liquid;
    energy          sensibleInternalEnergy;
}

mixture
{
    H2O;
    specie
    {
        molWeight   18.0;
    }
    equationOfState
    {
        R           3000; //1e10;
        rho0        1000;
    }
    thermodynamics
    {
        Cp          4180;
        Hf          0;
    }
    transport
    {
        mu          3.645e-4;
        Pr          2.289;
    }
}

